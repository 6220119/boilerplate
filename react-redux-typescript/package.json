{
  "name": "boilerplate-react-redux-typescript",
  "version": "0.0.1",
  "author": "Dmitry Sheiko (http://dsheiko.com)",
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "lint": "tslint --project tsconfig.json",
    "watch": "webpack --watch  --config webpack.config.js",
    "start": "http-server . -o -c-1"
  },
  "keywords": [
    "webpack",
    "react",
    "redux",
    "typescript",
    "starter",
    "boilerplate",
    "hot"
  ],
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "./jest-preprocessor.js"
    },
    "testRegex": "/.*\\.spec\\.(ts|tsx)$"
  },
  "devDependencies": {
    "@types/node": "^8.0.53",
    "@types/react": "^16.0.25",
    "@types/react-dom": "^16.0.3",
    "@types/react-redux": "^5.0.13",
    "@types/redux": "^3.6.0",
    "@types/redux-actions": "^2.2.2",
    "@types/redux-promise": "^0.5.28",
    "@types/redux-thunk": "^2.1.0",
    "clean-webpack-plugin": "^0.1.17",
    "http-server": "^0.10.0",
    "ts-loader": "^3.1.1",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1",
    "webpack": "^3.8.1"
  },
  "license": "ISC",
  "dependencies": {
    "material-ui": "^1.0.0-beta.21",
    "prop-types": "^15.6.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.2.0"
  }
}
